<ListTopicsResponse xmlns="http://sns.amazonaws.com/doc/2010-03-31/">
  <ListTopicsResult>
    <Topics>
      <% each_topic do |topic| %>
        <member>
          <TopicArn><%= topic.arn %></TopicArn>
        </member>
      <% end %>
    </Topics>
  </ListTopicsResult>
  <ResponseMetadata>
    <RequestId><%= request_id %></RequestId>
  </ResponseMetadata>
</ListTopicsResponse>
